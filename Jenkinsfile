pipeline {
    agent any
    stages {
        stage('Checking') {
            steps {
                sh 'ls -lah'
            }
        }
        stage('Building') {
            steps {
                sh 'echo building'
            }
        }
        stage('Testing') {
            steps {
                sh 'python '
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo deploying'
            }
        }
    }
}