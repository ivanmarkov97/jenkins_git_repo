pipeline {
    agent any
    stages {
        stage('Pulling') {
            steps {
                sh 'echo pulling from Git'
            }
        }
        stage('Building') {
            steps {
                sh 'echo building'
            }
        }
        stage('Testing') {
            steps {
                sh 'echo testing'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo deploying'
            }
        }
    }
}